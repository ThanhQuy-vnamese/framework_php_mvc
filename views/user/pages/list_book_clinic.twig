<!doctype html>
<html lang="en">

	<head>
		<!-- Required meta tags -->
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
		<title>medical</title>
		<!--Phần style, boostrap-->
		{{ include('user/components/head.twig') }}

	</head>

	<body>
		<!--::header part start::-->
		{{ include('user/components/menu.twig')}}
		<!-- Header part end-->

		<!--::doctor_part start::-->

			<div class="container"> <div class="row justify-content-center">
				<h3>Danh sách lịch khám</h3>
			</div>
			<section class="pt-5 pb-5">
				{% if(Session.hasFlash('makeAppointment')) %}
					<div class="alert alert-success">{{ session.getFlash('makeAppointment') }}</div>
				{% endif %}
				<div class="container">
					{% if list_book is empty %}
						<h1>Bạn chưa đặt lịch</h1>
					{% else %}

						<div class="row mb-md-2">
							{% for item in list_book %}
								<div class="col-md-6 col-lg-4">
									<div class="card shadow-sm border-light mb-4">
										<a href="#" class="position-relative">
											<img src="data:image/jpeg;base64," class="card-img-top" alt="image">
										</a>
										<div class="card-body">
											<a href="#">
												<h3 class="font-weight-normal">Họ tên:
													{{ item.full_name }}
												</h3>
											</a>
											<div class="d-flex my-4">
												<span class="small lh-120" style="font-size: 100%">
													<i class="fas fa-map-marker-alt mr-2"></i>Ngày hẹn: {{ item.date_start }}</span>
											</div>
											<div class="d-flex my-4">
												<span class="small lh-120" style="font-size: 100%">
													<i class="fas fa-map-marker-alt mr-2"></i>Địa chỉ: 456 khu 2 thị trấn Cái Bè</span>
											</div>
											<div class="d-flex my-4">
												<span class="small lh-120" style="font-size: 100%">
													<i class="fas fa-map-marker-alt mr-2"></i>Trạng thái: 
														{% if item.status == 0 %}
															Đang chờ duyệt
														{% elseif item.status == 1 %}
															Đã duyệt
														{% elseif item.status == 2 %}
															Hủy
														{% endif %}
													</span>
											</div>

										</div>
									</div>
								</div>
							{% endfor %}
						</div>

					{% endif %}
					<div class="row py-4 mt-md-5">
						<div class="col text-center">
							<a href="#" class="btn btn-lg shadow btn-primary mt-1">Browse all</a>
						</div>
					</div>
				</div>
			</section>
		</div>
		<!--::doctor_part end::-->


		<!-- footer part start-->
		{{ include('user/components/footer.twig')}}

		<!-- footer part end-->
	</body>
</html></body></html>
